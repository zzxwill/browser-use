# Action messages
msgid "🔍 Searched for \"{query}\" in Google"
msgstr "🔍 在谷歌搜索了\"{query}\""

msgid "🔗 Navigated to {url}"
msgstr "🔗 导航到 {url}"

msgid "🔙 Navigated back"
msgstr "🔙 已返回"

msgid "🕒 Waiting for {seconds} seconds"
msgstr "🕒 等待 {seconds} 秒"

msgid "🖱️ Clicked button with index {index}: {text}"
msgstr "🖱️ 点击了索引为 {index} 的按钮：{text}"

msgid "⌨️ Input {text} into index {index}"
msgstr "⌨️ 在索引 {index} 中输入了 {text}"

msgid "⌨️ Input sensitive data into index {index}"
msgstr "⌨️ 在索引 {index} 中输入了敏感数据"

msgid "💾 Downloaded file to {path}"
msgstr "💾 文件已下载到 {path}"

msgid "📄 Extracted from page"
msgstr "📄 从页面提取了内容"

msgid "🔄 Switched to tab {page_id}"
msgstr "🔄 切换到标签页 {page_id}"

msgid "🔗 Opened new tab with {url}"
msgstr "🔗 打开了新标签页 {url}"

msgid "❌ Closed tab #{page_id} with {url}, now focused on tab #{new_tab_idx} with url {new_url}"
msgstr "❌ 关闭了标签页 #{page_id} ({url})，现在聚焦于标签页 #{new_tab_idx} ({new_url})"

msgid "🔍 Scrolled down the page by {amount}"
msgstr "🔍 向下滚动页面 {amount}"

msgid "🔍 Scrolled up the page by {amount}"
msgstr "🔍 向上滚动页面 {amount}"

msgid "⌨️ Sent keys: {keys}"
msgstr "⌨️ 发送了按键：{keys}"

msgid "🔍 Scrolled to text: {text}"
msgstr "🔍 滚动到文本：{text}"

msgid "🖱️ Dragged from ({source_x}, {source_y}) to ({target_x}, {target_y})"
msgstr "🖱️ 从 ({source_x}, {source_y}) 拖拽到 ({target_x}, {target_y})"

msgid "🖱️ Dragged element '{source}' to '{target}'"
msgstr "🖱️ 将元素'{source}'拖拽到'{target}'"

msgid "New tab opened - switching to it"
msgstr "新标签页已打开 - 正在切换到该标签页"

msgid "Saving page with URL {url} as PDF to ./{filename}"
msgstr "将URL为 {url} 的页面保存为PDF到 ./{filename}"

# Agent messages
msgid "✅ Task completed successfully"
msgstr "✅ 任务成功完成"

msgid "❌ Task completed without success"
msgstr "❌ 任务完成但未成功"

msgid "⚡️ Decided next action: {action}"
msgstr "⚡️ 决定下一个动作：{action}"

msgid "⚡️ Decided next {count} multi-actions:"
msgstr "⚡️ 决定接下来的 {count} 个多重动作："

msgid "📍 Step {step}: Ran {count} actions in {duration:.2f}s: {status}"
msgstr "📍 步骤 {step}：在 {duration:.2f} 秒内执行了 {count} 个动作：{status}"

msgid "👍 Eval: {evaluation}"
msgstr "👍 评估：{evaluation}"

msgid "⚠️ Eval: {evaluation}"
msgstr "⚠️ 评估：{evaluation}"

msgid "❓ Eval: {evaluation}"
msgstr "❓ 评估：{evaluation}"

msgid "🧠 Memory: {memory}"
msgstr "🧠 记忆：{memory}"

msgid "🎯 Next goal: {goal}"
msgstr "🎯 下一个目标：{goal}"

msgid "❌ Result failed {failures}/{max_failures} times:"
msgstr "❌ 结果失败 {failures}/{max_failures} 次："

msgid "❌ Validator decision: {reason}"
msgstr "❌ 验证器决定：{reason}"

msgid "✅ Validator decision: {reason}"
msgstr "✅ 验证器决定：{reason}"

# Error messages
msgid "Element with index {index} does not exist - retry or use alternative actions"
msgstr "索引 {index} 的元素不存在 - 请重试或使用其他动作"

msgid "Element index {index} does not exist - retry or use alternative actions"
msgstr "索引 {index} 的元素不存在 - 请重试或使用其他动作"

msgid "Index {index} - has an element which opens file upload dialog. To upload files please use a specific function to upload files"
msgstr "索引 {index} - 有一个打开文件上传对话框的元素。要上传文件，请使用特定的文件上传功能"

msgid "Element not clickable with index {index} - most likely the page changed"
msgstr "索引 {index} 的元素不可点击 - 很可能页面已更改"

msgid "Browser closed or disconnected, unable to proceed"
msgstr "浏览器已关闭或断开连接，无法继续"

msgid "No visible element with text '{text}' found on page {url}"
msgstr "在页面 {url} 上找不到包含文本 '{text}' 的可见元素"

msgid "Text '{text}' not found or not visible on page"
msgstr "文本 '{text}' 未找到或在页面上不可见"

msgid "Failed to scroll to text '{text}': {error}"
msgstr "滚动到文本 '{text}' 失败：{error}"

msgid "Error getting options: {error}"
msgstr "获取选项时出错：{error}"

msgid "Cannot select option: Element with index {index} is a {tag_name}, not a select"
msgstr "无法选择选项：索引 {index} 的元素是 {tag_name}，不是选择框"

msgid "selected option {text} with value {values}"
msgstr "选择了选项 {text}，值为 {values}"

msgid "Could not select option '{text}' in any frame"
msgstr "无法在任何框架中选择选项 '{text}'"

msgid "Selection failed: {error}"
msgstr "选择失败：{error}"

msgid "Failed to find {element_type} element"
msgstr "找不到 {element_type} 元素"

msgid "Failed to determine {coord_type} coordinates"
msgstr "无法确定 {coord_type} 坐标"

msgid "Failed to perform drag and drop: {error}"
msgstr "执行拖拽操作失败：{error}"

# General quantities
msgid "one page"
msgstr "一页"

msgid "{amount} pixels"
msgstr "{amount} 像素"

msgid "source"
msgstr "源"

msgid "target"
msgstr "目标"

# System prompt additions for Chinese
msgid "IMPORTANT LANGUAGE INSTRUCTION:\n- You MUST communicate in Chinese (中文) for ALL your responses\n- Your evaluation_previous_goal, memory, and next_goal fields must be in Chinese\n- All extracted content and communication should be in Chinese\n- When encountering English websites, translate key information to Chinese for the user\n- Maintain professional and clear Chinese communication throughout"
msgstr "重要语言指令：\n- 您必须用中文回应所有内容\n- 您的evaluation_previous_goal、memory和next_goal字段必须用中文\n- 所有提取的内容和交流都应该用中文\n- 遇到英文网站时，将关键信息翻译成中文给用户\n- 在整个过程中保持专业和清晰的中文交流"

# Browser session messages
msgid "📐 Setting up viewport"
msgstr "📐 设置视口"

msgid "⚠️ Failed to grant browser permissions {permissions}: {error}"
msgstr "⚠️ 授予浏览器权限 {permissions} 失败：{error}"

msgid "❌ Launching new local browser {driver}:{channel} (v{version}) failed!"
msgstr "❌ 启动新的本地浏览器 {driver}:{channel} (v{version}) 失败！"

# Planner messages
msgid "🗺️ Planner enabled: {model} (interval={interval} steps)"
msgstr "🗺️ 规划器已启用：{model} (间隔={interval} 步骤)"

msgid "🗺️ Planner disabled"
msgstr "🗺️ 规划器已禁用"

msgid "🧠 Memory enabled: mem0 system with interval={interval} steps"
msgstr "🧠 记忆已启用：mem0 系统，间隔={interval} 步骤"

msgid "🧠 Memory disabled"
msgstr "🧠 记忆已禁用"

msgid "🎯 Max actions per step: {max_actions}"
msgstr "🎯 每步最大动作数：{max_actions}"

msgid "✅ Output validation enabled"
msgstr "✅ 输出验证已启用"

msgid "✅ Output validation disabled"
msgstr "✅ 输出验证已禁用"

# Configuration messages
msgid "⚠️⚠️⚠️ Agent(sensitive_data=••••••••) was provided but BrowserSession(allowed_domains=[...]) is not locked down! ⚠️⚠️⚠️"
msgstr "⚠️⚠️⚠️ 提供了Agent(sensitive_data=••••••••)但BrowserSession(allowed_domains=[...])未锁定！⚠️⚠️⚠️"

msgid "🧠 Starting a browser-use agent {version} with base_model={model}"
msgstr "🧠 启动browser-use代理 {version}，基础模型={model}"

# Missing messages from logs
msgid "🌎 Connecting to existing remote chromium-based browser over CDP: {url}"
msgstr "🌎 连接到现有远程基于Chromium的浏览器，通过CDP: {url}"

msgid "🌎 Using first browser_context available in existing browser: {context}"
msgstr "🌎 使用现有浏览器中第一个可用的browser_context: {context}"

msgid "Planning Analysis:"
msgstr "规划分析："

msgid "Error executing action {action}: {error}"
msgstr "执行动作 {action} 时出错：{error}"

msgid "[{task_id}] Agent initialized and ready to run"
msgstr "[{task_id}] 代理已初始化并准备运行"

msgid "[{task_id}] Agent started running"
msgstr "[{task_id}] 代理开始运行"

msgid "🌎 Launching local browser driver={driver} channel={channel} user_data_dir={user_data_dir}"
msgstr "🌎 启动本地浏览器 驱动={driver} 通道={channel} 用户数据目录={user_data_dir}"


# Additional agent step messages
msgid "🚀 Starting task: {task}"
msgstr "🚀 开始任务：{task}"

msgid "📍 Step {step}: Evaluating page with {count} interactive elements on: {url}"
msgstr "📍 步骤 {step}：正在评估页面，包含 {count} 个交互元素：{url}"

msgid "🧠 LLM call => {library} [✉️ {msg_count} msg, ~{tokens} tk, {chars} char{img}] {output}{tools}"
msgstr "🧠 LLM 调用 => {library} [✉️ {msg_count} 条消息，~{tokens} 令牌，{chars} 字符{img}] {output}{tools}"

msgid "☑️ Executed action {current}/{total}: {action}"
msgstr "☑️ 执行操作 {current}/{total}：{action}"

msgid "👁️ Foreground tab changed by human from [{old_idx}]{old_url} ➡️ [{new_idx}]{new_url} (agent will stay on [{agent_idx}]{agent_url})"
msgstr "👁️ 前台标签页由用户从 [{old_idx}]{old_url} 切换到 ➡️ [{new_idx}]{new_url} （代理将保持在 [{agent_idx}]{agent_url}）"

# Additional missing messages
msgid "Saving conversation to {path}"
msgstr "保存对话到 {path}"

msgid "🌎 Launching new local browser {browser} keep_alive={keep_alive} user_data_dir={user_data_dir}"
msgstr "🌎 启动新的本地浏览器 {browser} keep_alive={keep_alive} user_data_dir={user_data_dir}"

msgid "Element index changed after action {action} / {total}, because page changed."
msgstr "执行动作 {action} / {total} 后元素索引发生变化，因为页面已更改。"

msgid "🧠 LLM call => {library} [✉️ {msg_count} msg, ~{tokens} tk, {chars} char{image_status}] {output_format}{tool_info}"
msgstr "🧠 LLM 调用 => {library} [✉️ {msg_count} 条消息，~{tokens} 令牌，{chars} 字符{image_status}] {output_format}{tool_info}"

msgid " ↳ Spawned browser_pid={pid} {path}"
msgstr " ↳ 生成浏览器进程 browser_pid={pid} {path}" 